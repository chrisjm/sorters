{
    "compilerOptions": {
      "target": "esnext",
      "declaration": true,
      "sourceMap": true,
      "noImplicitAny": false,
      "noImplicitThis": false,
      "noEmit": true,
      "preserveConstEnums": true,
      "strict": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "tsout"
    },
    "include": ["src/**/*.ts"]
  }
